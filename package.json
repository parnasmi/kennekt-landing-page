{
  "name": "kennekt-landing-page",
  "version": "1.0.0",
  "description": "Landing page html/css of Kennekt CRM System",
  "main": "index.js",
  "scripts": {
    "sync": "browser-sync start --server --files \"**/*\"",
    "dev": "postcss index.css --output ./assets/styles/style.css ",
    "watch:dev": "nodemon -x npm run dev -w tailwind.config.js -w index.css -w index.html",
    "watch": "run-p watch:dev sync"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.2"
  }
}
